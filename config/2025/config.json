        {
          "title": "Starting Position",
          "type": "select",
          "required": true,
          "code": "Prsp",
          "defaultValue": "",
          "choices": {
            "LT": "Left Trench",
            "LB": "Left Bump",
            "H": "Hub",
            "RB": "Right Bump",
            "RT": "Right Trench"
          }
        },
        {
          "title": "No Show",
          "type": "boolean",
          "required": false,
          "code": "noShow",
          "defaultValue": false
        }
      ]
    },

    {
      "name": "Autonomous",
      "fields": [
