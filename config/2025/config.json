          "type": "number",
          "required": true,
          "code": "matchNumber",
          "defaultValue": 1,
          "formResetBehavior": "increment"
        },
        {
          "title": "Robot",
          "type": "select",
          "required": true,
          "code": "robot",
          "defaultValue": "R1",
          "choices": {
            "R1": "Red 1",
            "R2": "Red 2",
            "R3": "Red 3",
            "B1": "Blue 1",
            "B2": "Blue 2",
            "B3": "Blue 3"
          },
          "formResetBehavior": "preserve"
        },
        {
          "title": "Team Number",
          "type": "number",
          "required": true,
          "code": "teamNumber",
          "defaultValue": 0,
          "min": 0,
          "max": 19999
