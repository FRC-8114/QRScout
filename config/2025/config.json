        { "title": "Plays Defense in Alliance Zone", "type": "boolean", "required": false, "code": "S2DefAZ", "defaultValue": false }
      ]
    },

    {
      "name": "Shift 3",
      "fields": [
        { "title": "Hoppers Scored", "type": "counter", "required": false, "code": "S3HS", "defaultValue": 0, "min": 0, "step": 1 },
        { "title": "Passes Fuel from Neutral Zone", "type": "counter", "required": false, "code": "S3PFNZ", "defaultValue": 0, "min": 0, "step": 1 },
        { "title": "Passes Fuel from Opposing Alliance Zone", "type": "counter", "required": false, "code": "S3PFOAZ", "defaultValue": 0, "min": 0, "step": 1 },
        { "title": "Plays Defense in Neutral Zone", "type": "boolean", "required": false, "code": "S3DefNZ", "defaultValue": false },
        { "title": "Plays Defense in Alliance Zone", "type": "boolean", "required": false, "code": "S3DefAZ", "defaultValue": false }
