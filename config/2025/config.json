{
  "$schema": "https://frc2713.github.io/QRScout/schema.json",
  "title": "QRScout",
  "page_title": "Rebuilt Team 8114 -- 2026",
  "delimiter": "\t",
  "teamNumber": 8114,
  "sections": [
    {
      "name": "Prematch",
      "fields": [
        { "title": "Scouter Initials", "type": "text", "required": true, "code": "scouter", "defaultValue": "" },
        { "title": "Match Number", "type": "number", "required": true, "code": "matchNumber", "defaultValue": 1 },
        {
          "title": "Robot",
          "type": "select",
          "required": true,
          "code": "robot",
          "defaultValue": "R1",
          "choices": { "R1": "Red 1", "R2": "Red 2", "R3": "Red 3", "B1": "Blue 1", "B2": "Blue 2", "B3": "Blue 3" }
        },
        { "title": "Team Number", "type": "number", "required": true, "code": "teamNumber", "defaultValue": 0, "min": 0, "max": 19999 },
        {
          "title": "Starting Position",
          "type": "select",
          "required": true,
          "code": "startPos",
          "defaultValue": "H",
          "choices": {
            "OT": "Outpost Trench",
            "OB": "Outpost Bump",
            "H": "Hub",
            "DB": "Depot Bump",
            "DT": "Depot Trench"
          }
        },
        { "title": "No Show", "type": "boolean", "required": false, "code": "noShow", "defaultValue": false }
      ]
    },
    {
      "name": "Autonomous",
      "fields": [
        { "title": "Moved", "type": "boolean", "required": false, "code": "moved", "defaultValue": false },
        { "title": "Score Preload", "type": "boolean", "required": false, "code": "preload", "defaultValue": false },
        { "title": "Depot", "type": "boolean", "required": false, "code": "autonDepot", "defaultValue": false },
        { "title": "Outpost", "type": "boolean", "required": false, "code": "autonOutpost", "defaultValue": false },
        { "title": "Collects from Neutral Zone", "type": "boolean", "required": false, "code": "collectNZ", "defaultValue": false },
        { "title": "Auton Hoppers Scored", "type": "counter", "required": false, "code": "autonHoppers", "defaultValue": 0, "min": 0, "step": 1 },
        { "title": "Climb", "type": "boolean", "required": false, "code": "autonClimb", "defaultValue": false },
        {
          "title": "Climb Position",
          "type": "select",
          "required": false,
          "code": "autonClimbPos",
          "defaultValue": "Center",
          "choices": { "Outpost": "Outpost Side", "Center": "Center", "Depot": "Depot Side" }
        },
        { "title": "Crossed Lanes", "type": "boolean", "required": false, "code": "autonCrossLanes", "defaultValue": false },
        { "title": "Won Auton?", "type": "boolean", "required": false, "code": "wonAuto", "defaultValue": false }
      ]
    },
    {
      "name": "Teleop",
      "fields": [
        { "title": "Hoppers Scored", "type": "counter", "required": false, "code": "TeleopHoppers", "defaultValue": 0, "min": 0, "step": 1 },
       { "title": "Scores while Intaking", "type": "counter", "required": false, "code": "ShootIntake", "defaultValue": 0, "min": 0, "step": 1 },
        { "title": "Pass from Neutral Zone", "type": "counter", "required": false, "code": "shiftPassNZ", "defaultValue": 0, "min": 0, "step": 1 },
        { "title": "Crosses Bump", "type": "boolean", "required": false, "code": "shiftCrossBump", "defaultValue": false },
        { "title": "Crosses Trench", "type": "boolean", "required": false, "code": "shiftCrossTrench", "defaultValue": false },
        { "title": "Plays Defense in Alliance Zone", "type": "boolean", "required": false, "code": "shiftDefAlliance", "defaultValue": false },
        { "title": "Plays Defense in Neutral Zone", "type": "boolean", "required": false, "code": "shiftDefNeutral", "defaultValue": false }
      ]
    },
    {
      "name": "Endgame",
      "fields": [
        {
          "title": "Climb Level",
          "type": "select",
          "required": false,
          "code": "climbLevel",
          "defaultValue": "None",
          "choices": { "None": "None", "L1": "L1", "L2": "L2", "L3": "L3" }
        },
        {
          "title": "Climb Position",
          "type": "select",
          "required": false,
          "code": "endClimbPos",
          "defaultValue": "Center",
          "choices": { "Outpost": "Outpost Side", "Center": "Center", "Depot": "Depot Side" }
        }
      ]
    },
    {
      "name": "Postmatch",
      "fields": [
        {
          "title": "Balls per Hopper",
          "type": "select",
          "required": false,
          "code": "ballsPerHopper",
          "defaultValue": "21-30",
          "choices": {
            "<10": "<10",
            "11-20": "11-20",
            "21-30": "21-30",
            "31-40": "31-40",
            "41-50": "41-50",
            ">50": ">50"
          }
        },
        { "title": "Defensive Skill", "type": "range", "required": false, "code": "defSkill", "defaultValue": 3, "min": 1, "max": 5, "step": 1 },
        {
          "title": "Scoring Accuracy",
          "type": "select",
          "required": false,
          "code": "accuracy",
          "defaultValue": "M",
          "choices": { "L": "Low", "M": "Medium", "H": "High" }
        },
        { "title": "Passing Effectiveness", "type": "range", "required": false, "code": "passEffect", "defaultValue": 3, "min": 1, "max": 5, "step": 1 },
        { "title": "Defended?", "type": "boolean", "required": false, "code": "defended", "defaultValue": false },
        {
          "title": "Died?",
          "type": "select",
          "required": false,
          "code": "died",
          "defaultValue": "No",
          "choices": { "No": "No", "Com": "Communication Issues", "Brown": "Brown Out", "Disabled": "Mechanical Disabled" }
        },
        { "title": "Beached on Fuel", "type": "boolean", "required": false, "code": "beachedFuel", "defaultValue": false },
        { "title": "Beached on Bump", "type": "boolean", "required": false, "code": "beachedBump", "defaultValue": false },
        {
          "title": "Yellow / Red Card",
          "type": "select",
          "required": false,
          "code": "card",
          "defaultValue": "None",
          "choices": { "None": "No Card", "Yellow": "Yellow", "Red": "Red" }
        },
        { "title": "Comments", "type": "text", "required": false, "code": "comments", "defaultValue": "", "min": 0, "max": 50 }
      ]
    }
  ]
}

