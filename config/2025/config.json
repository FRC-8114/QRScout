        { "title": "Hoppers Scored", "type": "counter", "required": false, "code": "S4HS", "defaultValue": 0, "min": 0, "step": 1 },
        { "title": "Passes Fuel from Neutral Zone", "type": "counter", "required": false, "code": "S4PFNZ", "defaultValue": 0, "min": 0, "step": 1 },
        { "title": "Passes Fuel from Opposing Alliance Zone", "type": "counter", "required": false, "code": "S4PFOAZ", "defaultValue": 0, "min": 0, "step": 1 },
        { "title": "Plays Defense in Neutral Zone", "type": "boolean", "required": false, "code": "S4DefNZ", "defaultValue": false },
        { "title": "Plays Defense in Alliance Zone", "type": "boolean", "required": false, "code": "S4DefAZ", "defaultValue": false }
